<%= render ModalComponent.new do %>
	<%= form_for @club, authenticity_token: true, data: {controller: "mandatory-fields", turbo_frame: "_top"} do |form| %>
		<% @title.form = form %>
		<% @fields.form = form %>
		<%= render @title %>
		<%= render @fields %>
		<%= render @submit %>
	<% end %>
<% end %>